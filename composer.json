{
    "name": "wigilabs/wigilabs-backend-proxy",
    "type": "project",
    "require": {
        "php": "^7.4",
        "codeigniter4/framework": "^4",
        "slim/slim": "^4.8",
        "guzzlehttp/guzzle": "^7.4",
        "monolog/monolog": "^2.3",
        "predis/predis": "^1.1",
        "vlucas/phpdotenv": "^5.4",
        "ext-json": "*",
        "ext-soap": "*",
        "slim/psr7": "^1.7"
    },
    "autoload": {
        "psr-4": {
            "Wigilabs\\CodeIgniter\\": "src/CodeIgniter/app/",
            "Wigilabs\\Slim\\": "src/Slim/src/",
            "Wigilabs\\Common\\": "src/Common/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\CodeIgniter\\": "tests/CodeIgniter/",
            "Tests\\Slim\\": "tests/Slim/",
            "Tests\\Common\\": "tests/Common/"
        }
    }
}
